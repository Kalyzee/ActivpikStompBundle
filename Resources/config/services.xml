<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="activpik_stomp" class="Activpik\StompBundle\Connectors\StompConnector">
            <argument>%activpik_stomp.configuration%</argument>
        </service>
        <service id="activpik_stomp_message_factory" class="Activpik\StompBundle\Message\MessageFactory">
        </service>        
    </services>
    
</container>
